# git alias land
g ()
{
	arg1=$1
	shift
	case "$arg1" in
		a)
			eval git add $@;;
		b)
			eval git branch $@;;
		c)
			eval git commit -m \"$1\";;
		co)
			eval git checkout $@;;
		d)
			eval git diff $@;;
		f)
			eval git fetch $@;;
		i)
			eval git init $@;;
		m)
			eval git merge $@;;
		pll)
			eval git pull $@;;
		psh)
			eval git push $@;;
		rst)
			eval git reset $@;;
		rsto)
			eval git restore $@;;
		s)
			eval git status $@;;
		*)
			eval git $arg1 $@;;
	esac
}
